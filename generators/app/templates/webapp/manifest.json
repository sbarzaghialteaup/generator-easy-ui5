{
    "_version": "1.12.0",
    "sap.app": {
        "id": "<%=namespace%>.<%=appname%>",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "CAP_Services": {
                "uri": "/srv_api/services/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "title": "Manage <%=entitynameplural%>",
        "description": "<%=entitynameplural%> Maintenance",
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "<%=entitynameplural%>",
                    "action": "manage",
                    "title": "Manage <%=entitynameplural%>",
                    "description": "<%=entitynameplural%> Maintenance",
                    "icon": "sap-icon://add"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": true,
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.78.0",
            "libs": {
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "<%=namespace%>.<%=appname%>.i18n.i18n"
                }
            },
            "": {
                "dataSource": "CAP_Services",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "<%=entitynameplural%>List",
                    "target": "<%=entitynameplural%>List"
                },
                {
                    "pattern": "<%=entitynameplural%>({key}):?query:",
                    "name": "<%=entitynamesingolar%>Details",
                    "target": "<%=entitynamesingolar%>Details"
                }
            ],
            "targets": {
                "<%=entitynameplural%>List": {
                    "type": "Component",
                    "id": "<%=entitynameplural%>List",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "<%=entitynameplural%>",
                            "navigation": {
                                "<%=entitynameplural%>": {
                                    "detail": {
                                        "route": "<%=entitynamesingolar%>Details"
                                    }
                                }
                            }
                        }
                    }
                },
                "<%=entitynamesingolar%>Details": {
                    "type": "Component",
                    "id": "<%=entitynamesingolar%>Details",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "<%=entitynameplural%>"
                        }
                    }
                }
            }
        }
    }
}
